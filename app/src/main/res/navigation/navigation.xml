<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigation"
    app:startDestination="@id/titleFragment">

    <fragment
        android:id="@+id/titleFragment"
        android:name="com.momomomo111.camerax_opencv.ui.TitleFragment"
        android:label="Title">
        <action
            android:id="@+id/action_titleFragment_to_gaussianBlurFragment"
            app:destination="@id/gaussianBlurFragment" />
        <action
            android:id="@+id/action_titleFragment_to_thresholdFragment"
            app:destination="@id/thresholdFragment" />
        <action
            android:id="@+id/action_titleFragment_to_licenceFragment"
            app:destination="@id/licenceFragment" />
        <action
            android:id="@+id/action_titleFragment_to_cannyFragment"
            app:destination="@id/cannyFragment" />
        <action
            android:id="@+id/action_titleFragment_to_grayScaleFragment"
            app:destination="@id/grayScaleFragment" />
        <action
            android:id="@+id/action_titleFragment_to_rgbExtractionFragment"
            app:destination="@id/rgbExtractionFragment" />
        <action
            android:id="@+id/action_titleFragment_to_hsvExtractionFragment"
            app:destination="@id/hsvExtractionFragment" />
        <action
            android:id="@+id/action_titleFragment_to_erodeFragment"
            app:destination="@id/erodeFragment" />
        <action
            android:id="@+id/action_titleFragment_to_dilateFragment"
            app:destination="@id/dilateFragment" />
    </fragment>
    <fragment
        android:id="@+id/gaussianBlurFragment"
        android:name="com.momomomo111.camerax_opencv.ui.GaussianBlurFragment"
        android:label="GaussianBlur">
        <argument
            android:name="vibrationEnable"
            android:defaultValue="false"
            app:argType="boolean" />
    </fragment>
    <fragment
        android:id="@+id/thresholdFragment"
        android:name="com.momomomo111.camerax_opencv.ui.ThresholdFragment"
        android:label="Threshold">
        <argument
            android:name="vibrationEnable"
            android:defaultValue="false"
            app:argType="boolean" />
    </fragment>
    <fragment
        android:id="@+id/licenceFragment"
        android:name="com.momomomo111.camerax_opencv.ui.LicenceFragment"
        android:label="Licence">
        <argument
            android:name="vibrationEnable"
            android:defaultValue="false"
            app:argType="boolean" />
    </fragment>
    <fragment
        android:id="@+id/cannyFragment"
        android:name="com.momomomo111.camerax_opencv.ui.CannyFragment"
        android:label="Canny">
        <argument
            android:name="vibrationEnable"
            android:defaultValue="false"
            app:argType="boolean" />
    </fragment>
    <fragment
        android:id="@+id/grayScaleFragment"
        android:name="com.momomomo111.camerax_opencv.ui.GrayScaleFragment"
        android:label="GrayScale"/>
    <fragment
        android:id="@+id/rgbExtractionFragment"
        android:name="com.momomomo111.camerax_opencv.ui.RgbExtractionFragment"
        android:label="RgbExtraction">
        <argument
            android:name="vibrationEnable"
            android:defaultValue="false"
            app:argType="boolean" />
    </fragment>
    <fragment
        android:id="@+id/hsvExtractionFragment"
        android:name="com.momomomo111.camerax_opencv.ui.HsvExtractionFragment"
        android:label="HsvExtraction">
        <argument
            android:name="vibrationEnable"
            android:defaultValue="false"
            app:argType="boolean" />
    </fragment>
    <fragment
        android:id="@+id/erodeFragment"
        android:name="com.momomomo111.camerax_opencv.ui.ErodeFragment"
        android:label="Erode">
        <argument
            android:name="vibrationEnable"
            android:defaultValue="false"
            app:argType="boolean" />
    </fragment>
    <fragment
        android:id="@+id/dilateFragment"
        android:name="com.momomomo111.camerax_opencv.ui.DilateFragment"
        android:label="Dilate">
        <argument
            android:name="vibrationEnable"
            android:defaultValue="false"
            app:argType="boolean" />
    </fragment>
</navigation>